  "bugs": {
    "url": "https://github.com/CodeChris96/music-library/issues"
  },
  "homepage": "https://github.com/CodeChris96/music-library#readme",
  "devDependencies": {
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "postgres-migrations": "^5.3.0"
  }
